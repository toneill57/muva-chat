{
  "name": "muva-chat",
  "version": "0.1.0",
  "private": true,
  "description": "Multi-tenant chat platform with SIRE compliance for tourism businesses",
  "overrides": {
    "axios": "^1.8.0",
    "inflight": "npm:lru-cache@^11.0.0",
    "test-exclude": "^7.0.1"
  },
  "scripts": {
    "benchmark-detailed": "node scripts/detailed-benchmark.js",
    "build": "next build --turbopack",
    "clean": "rm -rf .next node_modules/.cache data/temp logs/*.log && echo '✅ Cache limpiado'",
    "clean:deep": "pnpm run clean && rm -rf node_modules && echo '✅ Reinstalando dependencias...' && pnpm install",
    "clear-database": "node scripts/clear-database.js",
    "deploy:dev": "./scripts/deploy-dev.sh",
    "deploy:staging": "./scripts/deploy-staging.sh",
    "dev": "pnpm run kill-port && next dev --turbopack",
    "dev:production": "./scripts/dev-production.sh",
    "dev:staging": "./scripts/dev-staging.sh",
    "dev:force": "pnpm run kill-all && next dev --turbopack",
    "ensure-server": "node scripts/ensure-server.js",
    "env:production": "./scripts/toggle-env.sh",
    "env:staging": "./scripts/toggle-env.sh",
    "kill-all": "pkill -f 'next dev' 2>/dev/null || true && pkill -f 'node.*next' 2>/dev/null || true && pnpm run kill-port && pnpm run kill-port:staging",
    "kill-port": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true",
    "kill-port:staging": "lsof -ti:3001 | xargs kill -9 2>/dev/null || true",
    "lint": "eslint",
    "monitor": "node scripts/system-monitor.js",
    "populate-embeddings": "node scripts/populate-embeddings.js",
    "pre-deploy": "pnpm run validate-env && pnpm run lint && pnpm run build",
    "prebuild": "pnpm dlx tsx scripts/build-welcome-message.ts",
    "remap:manual-ids": "tsx scripts/smart-remap-manual-ids.ts",
    "setup-pgvector": "node scripts/setup-vector-function.js",
    "start": "next start",
    "sync:motopress:bookings": "tsx scripts/sync-motopress-bookings.ts",
    "test": "jest",
    "test-performance": "node scripts/test-performance.js",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:rpc": "jest __tests__/database/rpc-functions.test.ts",
    "validate:rpc": "tsx scripts/validate-rpc-functions.ts",
    "validate:rpc:fix": "tsx scripts/validate-rpc-functions.ts --fix",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:setup": "tsx e2e/setup/test-database-setup.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "jest --watch",
    "validate-env": "./scripts/validate-env.sh"
  },
  "type": "module",
  "pnpm": {
    "overrides": {
      "glob@7.2.3": "^11.0.0",
      "inflight": "npm:lru-cache@^11.0.0",
      "formdata-node@4.4.1": "^6.0.3"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "puppeteer",
      "sharp",
      "unrs-resolver"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@langchain/community": "^1.0.0",
    "@langchain/core": "^1.0.2",
    "@langchain/openai": "^1.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.77.0",
    "@tiptap/react": "^3.9.1",
    "@tiptap/starter-kit": "^3.9.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/leaflet": "^1.9.21",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.24",
    "fuse.js": "^7.1.0",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "html2canvas": "^1.4.1",
    "jose": "^6.1.0",
    "langchain": "^0.3.34",
    "leaflet": "^1.9.4",
    "lru-cache": "^11.0.2",
    "lucide-react": "^0.548.0",
    "moment": "^2.30.1",
    "next": "15.5.3",
    "node-ical": "^0.22.1",
    "openai": "^6.7.0",
    "pdfjs-dist": "^5.4.296",
    "pg": "^8.16.3",
    "puppeteer": "^24.27.0",
    "react": "^19.2.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-intersection-observer": "^10.0.0",
    "react-leaflet": "^5.0.0",
    "react-markdown": "^10.1.0",
    "react-pdf": "^10.2.0",
    "recharts": "^3.3.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tiktoken": "^1.0.22",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.18",
    "@types/react": "^19.2.2",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.5.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
